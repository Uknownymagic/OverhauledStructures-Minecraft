          "functions": [
            {
              "function": "minecraft:set_nbt",
              "tag": "{display:{Name:'[{\"text\":\"Lost Note\",\"italic\":true,\"bold\":true,\"color\":\"aqua\"}]',Lore:['[{\"text\":\"If you see this, I have been eaten.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"We were digging out the tunnels when\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"our limbs froze up.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Invisible tethers holding us back.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Strings wrapped around our arms, our\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"legs, our head.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"I escaped, barely.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Saved by a pocket knife.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"The others are gone.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"I\\'m sorry.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"I\\'m going to try to stop this.\",\"italic\":false,\"color\":\"aqua\"}]']}}"
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 2,
          "name": "minecraft:book",
          "functions": [
            {
              "function": "minecraft:set_nbt",
              "tag": "{display:{Name:'[{\"text\":\"Lost Journal\",\"italic\":true,\"bold\":true,\"color\":\"aqua\"}]',Lore:['[{\"text\":\"Which is better?\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Soup, or pie?\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Or cake?\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"I\\'m thinking of pie.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Because somehow, down here, there are a lot of pies.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Why?\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"They\\'re preserved. Well-stored. For a person.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"This is a spider infested mess, and there are pies.\",\"italic\":false,\"color\":\"aqua\"}]']}}"
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 2,
          "name": "minecraft:book",
          "functions": [
            {
              "function": "minecraft:set_nbt",
              "tag": "{display:{Name:'[{\"text\":\"Lost Journal\",\"italic\":true,\"bold\":true,\"color\":\"aqua\"}]',Lore:['[{\"text\":\"A local villager informed me of a caving system nearby.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Said spiders had been heard for weeks.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"Offered to pay a grand sum if I cleared it out.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"The children deserve the rest.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"I\\'ll be back home in a couple weeks.\",\"italic\":false,\"color\":\"aqua\"}]','[{\"text\":\"In and out, safe and sound.\",\"italic\":false,\"color\":\"aqua\"}]']}}"
            }
          ]
        },
